{
  "name": "mcre-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run api:build && concurrently \"vite\" \"npm run api:watch\"",
    "build": "npm run sitemap:build && vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "api:build": "rm -rf ./tmp-apis && npx openapi2aspida -o=./tmp-apis/ && rsync -a --delete ./tmp-apis/ ./src/apis/ && rm -rf ./tmp-apis",
    "api:watch": "chokidar ./openapi.yaml -c \"npm run api:build\"",
    "sitemap:build": "node ./bin/updateSitemap.js"
  },
  "dependencies": {
    "@aspida/fetch": "^1.14.0",
    "@fontsource-variable/roboto-flex": "^5.1.0",
    "aspida": "^1.14.0",
    "core-js": "^3.37.1",
    "dayjs": "^1.11.13",
    "vue": "^3.4.31",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@mdi/js": "^7.4.47",
    "@types/node": "^20.14.10",
    "@unhead/vue": "^1.11.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "critters": "^0.0.24",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-vuetify": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "1.77.6",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vite-ssg": "^0.23.8",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.0.26"
  }
}
